<?xml version='1.0' encoding='utf-8'?>
<oor:component-data oor:name="Addons" oor:package="org.openoffice.Office"
                    xmlns:oor="http://openoffice.org/2001/registry"
                    xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <node oor:name="AddonUI">
        <node oor:name="OfficeMenuBar">
            <node name="{{cookiecutter.package_name}}" oor:op="replace">
                <prop name="Context" oor:type="xs:string">
                    <value/>
                </prop>
                <prop name="Title" oor:type="xs:string">
                    <value oor:lang="fr">{{cookiecutter.company_name}}</value>
                </prop>
                <node oor:name="Submenu">
                    <node oor:name="N001" oor:op="replace">
                        <prop name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                        <prop name="Title" oor:type="xs:string">
                            <value oor:lang="fr">
                                {{cookiecutter.extension_label}}
                            </value>
                        </prop>
                        <prop name="URL" oor:type="xs:string">
                            <value>
                                vnd.sun.star.script:{{cookiecutter.extension_name}}-{{cookiecutter.extension_version}}.oxt|python|{{cookiecutter.extension_name}}.py${{cookiecutter.extension_name}}_launcher?language=Python&amp;amp;location=user:uno_packages
                            </value>
                        </prop>
                        <prop name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                    </node>
                    <node oor:name="N002" oor:op="replace">
                        <prop name="Context" oor:type="xs:string">
                            <value>com.sun.star.text.TextDocument</value>
                        </prop>
                        <prop name="Title" oor:type="xs:string">
                            <value oor:lang="fr">tatar</value>
                        </prop>
                        <prop name="URL" oor:type="xs:string">
                            <value>
                                vnd.sun.star.script:{{cookiecutter.extension_name}}-{{cookiecutter.extension_version}}.oxt|python|{{cookiecutter.extension_name}}.py${{cookiecutter.extension_name}}_launcher?language=Python&amp;amp;location=user:uno_packages
                            </value>
                        </prop>
                        <prop name="Target" oor:type="xs:string">
                            <value>_self</value>
                        </prop>
                    </node>
                </node>
            </node>
        </node>
        <node oor:name="OfficeToolBar"/>
        <node oor:name="Images"/>
    </node>
</oor:component-data>